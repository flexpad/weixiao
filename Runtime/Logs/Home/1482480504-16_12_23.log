SQL: INSERT INTO `wp_custom_reply_news` (`keyword`,`keyword_type`,`title`,`intro`,`cate_id`,`cover`,`content`,`sort`,`jump_url`,`author`,`cTime`,`token`) VALUES ('考场作文之片语只言','0','考场作文之片语只言','关注自我的发展——素质、成长、交往','19','1151','<p><span style=\"font-family: 宋体; font-size: 19px;\"><br/></span></p><p><span style=\"font-family: 宋体; font-size: 19px;\">一、考场作文备考类别</span><br/></p><p><span style=\"font-family: 宋体; font-size: 19px;\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">关注自我的发展——素质、成长、交往类</span></p><p><span style=\"font-size:19px;font-family:宋体\">体验人间的真情——亲情、友情、爱国类</span></p><p><span style=\"font-size:19px;font-family:宋体\">放飞想像的翅膀——观察、想像、思考类</span></p><p><span style=\"font-size:19px;font-family:宋体\">感受实践的苦乐——发现、探索、成功类</span></p><p><span style=\"font-size:19px;font-family:宋体\">倾吐心灵的企盼——志向、理想、前途类</span></p><p><span style=\"font-size:19px;font-family:宋体\">陶冶美好的情操——言行、修养、美德类</span></p><p><span style=\"font-size:19px;font-family:宋体\">发现学习的兴趣——读书、求知、方法类</span></p><p><span style=\"font-size:19px;font-family:宋体\">紧扣时代的脉搏——环保、科技、时尚类</span></p><p><span style=\"font-size:19px;font-family:宋体\">品味生活的真谛——生活、社会、时事类</span></p><p><span style=\"font-size:19px;font-family:宋体\">推动历史的车轮——历史、现实、未来类</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">二、考场作文“五个一”</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p style=\"text-align: left;\"><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">文面——披上一件华丽的外衣</span></p><p style=\"text-align: left;\"><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">标题——树起一面鲜艳的旗帜</span></p><p style=\"text-align: left;\"><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">开头——推出一位靓丽的新娘</span></p><p style=\"text-align: left;\"><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">主体——构建一个匀称的酮体</span></p><p style=\"text-align: left;\"><span style=\"font-size:19px;font-family:宋体\">    结尾——画上一个圆满的句号</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">三、考场作文“四亮点”</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">亮点一：浓郁的生活气息</span></p><p><span style=\"font-size:19px;font-family:宋体\">亮点二：精美的诗词典故</span></p><p><span style=\"font-size:19px;font-family:宋体\">亮点三：出色的景物描写</span></p><p><span style=\"font-size:19px;font-family:宋体\">亮点四：美观的篇章结构</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">四、考场作文六个“将”</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">将文题审准</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">将短话写长</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">将结构写活</span></p><p><span style=\"font-size:19px;font-family:宋体\">将语言写美</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">将道理写透</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">将情意写浓</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">五、作文写好三段话</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">开头段</span></p><p><span style=\"font-size:19px;font-family:宋体\">突破一个“巧”字，做到别开生面，抓住读者</span></p><p><span style=\"font-size:19px;font-family:宋体\">（打动读者）</span></p><p><span style=\"font-size:19px;font-family:宋体\">中间段</span></p><p><span style=\"font-size:19px;font-family:宋体\">突破一个“活”字，做到别有情趣，感动读者</span></p><p><span style=\"font-size:19px;font-family:宋体\">（说服读者）</span></p><p><span style=\"font-size:19px;font-family:宋体\">结尾段</span></p><p><span style=\"font-size:19px;font-family:宋体\">突破一个“妙”字，做到别具一格，留住读者</span></p><p><span style=\"font-size:19px;font-family:宋体\">（启迪读者）</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">六、考场作文的构思</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">抓准一条线儿（立意深刻）</span></p><p><span style=\"font-size:19px;font-family:宋体\">把线砍几段儿（逻辑分段）</span></p><p><span style=\"font-size:19px;font-family:宋体\">段要锯为片儿（小段连缀）</span></p><p><span style=\"font-size:19px;font-family:宋体\">最后穿成串儿（扣题升华）</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">七、考场作文的“累累伤痕”</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">一、标题残破</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">二、卷面脏乱</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">三、字数不足</span></p><p><span style=\"font-size:19px;font-family:宋体\">四、别字连篇</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">五、标点乱用</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">六、文体不明</span></p><p><span style=\"font-size:19px;font-family:宋体\">七、常识错误</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">八、阅卷老师看你考场作文的什么</span></p><p><span style=\"font-size:19px;font-family:宋体\"><br/></span></p><p><span style=\"font-size:19px;font-family:宋体\">看你的行文是否扣题</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">看你的思路是否清晰</span></p><p><span style=\"font-size:19px;font-family:宋体\">看你的表达是否精彩</span><span style=\"font-size:19px\">    </span><span style=\"font-size:19px;font-family:宋体\">看你的立意是否出新</span></p><p><img src=\"/weiphp30/Uploads/Editor/gh_2837e31e28ed/2016-12-23/585ccf741a1b3.jpg\" style=\"float:none;\" title=\"大.jpg\"/></p><p><img src=\"/weiphp30/Uploads/Editor/gh_2837e31e28ed/2016-12-23/585ccf746b401.png\" style=\"float:none;\" title=\"643653605D9909F0B6AA915DCD18A2B7.png\"/></p>','0','','','1482477433','gh_2837e31e28ed') [ RunTime:0.0003s ]
